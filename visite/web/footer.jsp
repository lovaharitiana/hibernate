<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>Gestion de Médecin - Footer</title>
    <style>
        * {
            margin: 0;
            padding: 0;
        }

        html, body {
            height: 100%;
        }

        .wrapper {
            min-height: 100%;
            position: relative;
        }

        .content {
            padding-bottom: 60px; /* Hauteur de la footer */
        }

        .footer {
            position: absolute;
            bottom: 0;
            width: 100%;
            height: 40px; /* Hauteur de la footer */
            background-color: #f2f2f2;
            padding: 10px;
            text-align: center;
        }

        .footer p {
            margin: 0;
        }
    </style>
</head>
<body>
    <div class="wrapper">
        

        <div class="footer">
            <p>© 2023 Votre Application de Gestion de Médecin.</p>
        </div>
    </div>
</body>
</html>
